{
    "name": "entreya/csvquery",
    "description": "High-performance CSV query engine with Go-powered indexing and Yii2-like fluent API",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "csv",
        "query",
        "database",
        "indexing",
        "performance",
        "big-data",
        "active-query",
        "simd"
    ],
    "authors": [
        {
            "name": "Entreya Team",
            "email": "iamhimanshu7102@gmail.com"
        }
    ],
    "homepage": "https://github.com/entreya/csvquery",
    "support": {
        "issues": "https://github.com/entreya/csvquery/issues",
        "source": "https://github.com/entreya/csvquery"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.1",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "suggest": {
        "ext-posix": "For Unix Domain Socket support (recommended)"
    },
    "autoload": {
        "psr-4": {
            "Entreya\\CsvQuery\\": "src/php/",
            "Entreya\\CsvQuery\\Core\\": "src/php/Core/",
            "Entreya\\CsvQuery\\Query\\": "src/php/Query/",
            "Entreya\\CsvQuery\\Bridge\\": "src/php/Bridge/",
            "Entreya\\CsvQuery\\Models\\": "src/php/Models/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Entreya\\CsvQuery\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "build": "php scripts/build.php",
        "build:all": "php scripts/build.php --all",
        "build:clean": "php scripts/build.php --clean",
        "post-install-cmd": [
            "@php scripts/build.php 2>&1 || echo 'Note: Go build skipped. Run composer build manually if needed.'"
        ],
        "post-update-cmd": [
            "@php scripts/build.php 2>&1 || echo 'Note: Go build skipped. Run composer build manually if needed.'"
        ]
    },
    "config": {
        "sort-packages": true
    }
}
